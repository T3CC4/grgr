<% if (typeof message !== 'undefined' && message && message !== null) { %>
    <div class="alert alert-<%= typeof messageType !== 'undefined' ? messageType : 'info' %> alert-dismissible fade show" role="alert">
        <%= message %>
        <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
    </div>
<% } %>